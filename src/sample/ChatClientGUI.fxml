<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="618.0" prefWidth="464.0" xmlns="http://javafx.com/javafx/8.0.231" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ChatClientGUIController">
      <Pane prefHeight="71.0" prefWidth="464.0">
            <Button fx:id="save" layoutX="396.0" layoutY="16.0" mnemonicParsing="false" onAction="#save" text="Save" />
            <Button fx:id="exit" layoutX="28.0" layoutY="16.0" mnemonicParsing="false" onAction="#exit" prefHeight="25.0" prefWidth="49.0" text="Exit" />
      </Pane>
      <Pane prefHeight="487.0" prefWidth="464.0">
            <TextArea fx:id="messageLog" layoutX="27.0" layoutY="14.0" prefHeight="458.0" prefWidth="410.0" />
      </Pane>
      <Pane prefHeight="77.0" prefWidth="464.0">
            <Button fx:id="file" layoutX="27.0" layoutY="19.0" mnemonicParsing="false" onAction="#file" text="File" />
            <TextField fx:id="textMessage" layoutX="76.0" layoutY="14.0" prefHeight="34.0" prefWidth="307.0" promptText="Write something here...">
               <font>
                  <Font name="Arial" size="13.0" />
               </font>
            </TextField>
            <Button fx:id="send" layoutX="395.0" layoutY="19.0" mnemonicParsing="false" onAction="#send" text="Send" />
      </Pane>
</VBox>
